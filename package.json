{
  "scripts": {
    "build": "npm run build:leaner && npm run build:duplicates",
    "build:leaner": "npx tailwindcss -i ./src/leaner.css -o ./dist/leaner.css --minify",
    "build:duplicates": "npx tailwindcss -i ./src/duplicates.css -o ./dist/duplicates.css --minify",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.7",
    "gh-pages": "^4.0.0",
    "tailwindcss": "^3.1.8"
  }
}